<?xml version="1.0" encoding="utf-8"?>
<Defs>

    <!-- The chapter material for the Black Templars, this is what is minimum needed to have it function properly -->
    <ThingDef ParentName="BEWH_ChapterGeneticMaterial">
        <defName>BEWH_ChapterMaterialBlackTemplar</defName>
        <label>chapter material - Black Templars</label>
        <description>A specialized container designed to store and preserve the sacred genetic data of a specific Astartes Legion or Chapter. This material develops within a Space Marine's body and can be harvested once fully developed and retrieved from the fallen.\n\nThis unit holds the genetic mutation material of Chapter - Black Templars.</description>
        <graphicData>
            <texPath>Things/Item/ChapterMaterial/BEWH_ChapterMaterial_BlackTemplars</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <descriptionHyperlinks>
            <GeneDef>BEWH_ChapterBlackTemplars</GeneDef>
        </descriptionHyperlinks>
        <modExtensions>
            <li Class="Genes40k.DefModExtension_ChapterMaterial">
                <orderInt>5500</orderInt>
                <shownMaterialName>Black Templars</shownMaterialName>
            </li>
            <li Class="Genes40k.DefModExtension_GeneFromMaterial">
                <addedGene>BEWH_ChapterBlackTemplars</addedGene>
            </li>
        </modExtensions>
    </ThingDef>

</Defs>