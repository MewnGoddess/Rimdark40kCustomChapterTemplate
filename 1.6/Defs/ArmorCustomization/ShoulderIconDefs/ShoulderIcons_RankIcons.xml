<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <!-- Black Templar rank icon available to anyone with the Neophyte rank -->
    <Genes40k.ShoulderIconDef ParentName="BEWH_RankShoulderIcon">
        <defName>BEWH_RankIconBlackTemplar</defName>
        <label>Black Templar</label>
        <description>Black Templar rank icon.</description>
        <iconPath>UI/Decoration/LegionBadges/BEWH_iconUI_Chapter_BlackTemplars</iconPath>
        <drawnTextureIconPath>Things/Armor/Imperium/PowerArmor/RankIcons/BEWH_BTRankShoulder</drawnTextureIconPath>
        <sortOrder>99</sortOrder>
        <useColour>true</useColour>        <!-- This allows player to colour their icons-->
        <defaultColour>(44, 44, 44)</defaultColour>        <!-- This sets the default colour of the icon (Defaults to white if null) -->
        <mustHaveRank>
            <li>BEWH_AstartesRankNeophyte</li>
        </mustHaveRank>
    </Genes40k.ShoulderIconDef>

</Defs>